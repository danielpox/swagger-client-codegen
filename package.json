{
  "name": "swagger-client-codegen",
  "version": "0.1.3",
  "description": "Generate a TypeScript API client or GraphQL schema based on a Swagger JSON file",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielpox/swagger-client-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/danielpox/swagger-client-codegen/issues"
  },
  "homepage": "https://github.com/danielpox/swagger-client-codegen#readme",
  "keywords": [
    "swagger",
    "api",
    "client",
    "codegen",
    "typescript",
    "graphql"
  ],
  "main": "lib/swagger-client-codegen.js",
  "types": "lib/interfaces.d.ts",
  "bin": {
    "swagger-client-codegen": "bin/index.js"
  },
  "scripts": {
    "start": "npm run generate",
    "generate": "node lib/swagger-client-codegen.js",
    "prepublish": "npm run build",
    "build": "tsc"
  },
  "dependencies": {
    "axios": "0.18.0",
    "chalk": "2.4.1",
    "fs": "0.0.1-security",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "@types/node": "10.12.12",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  },
  "peerDependencies": {
    "apisauce": "^1.x.x"
  }
}
